{
  "name": "wechat-ai",
  "version": "0.0.1",
  "description": "Wechat with Ai.",
  "author": {
    "name": "huchundong",
    "url": "https://github.com/huchundong"
  },
  "repository": "https://github.com/huchundong/wechat-ai",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.mjs development",
    "test": "node scripts/dev-server.mjs test",
    "production": "node scripts/dev-server.mjs production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "new:window": "node scripts/create-window.mjs",
    "new:ipc": "node scripts/create-ipc.mjs",
    "build": "node scripts/build.mjs && electron-forge make",
    "build:win32": "node scripts/build.mjs && electron-forge make --platform=win32 --arch=ia32",
    "build:win64": "node scripts/build.mjs && electron-forge make --platform=win32 --arch=x64",
    "build:mac": "node scripts/build.mjs && electron-forge make --platform=darwin",
    "build:linux": "node scripts/build.mjs && electron-forge make --platform=linux",
    "build:nsis-win32": "node scripts/build.mjs && electron-forge package --platform=win32 --arch=ia32 && ./setup/NSIS/nsis-3.08/makensis.exe ./setup/NSIS/win-setup-x86.nsi",
    "build:nsis-win64": "node scripts/build.mjs && electron-forge package --platform=win32 --arch=x64 && ./setup/NSIS/nsis-3.08/makensis.exe ./setup/NSIS/win-setup-x64.nsi"
  },
  "dependencies": {
    "@electron-forge/plugin-base": "^7.7.0",
    "axios": "^1.8.1",
    "electron-clipboard-ex": "^1.3.3",
    "electron-log": "^5.3.1",
    "ffmpeg-static-electron-forge": "^1.1.7",
    "fluent-ffmpeg": "^2.1.3",
    "mqtt": "^5.10.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@imengyu/vue3-context-menu": "^1.4.5",
    "@types/html-to-text": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vueuse/core": "^12.8.2",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.4",
    "electron": "^35.0.0",
    "eslint": "8.57",
    "eslint-plugin-vue": "^9.32.0",
    "file-type": "^20.4.0",
    "html-to-text": "^9.0.5",
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "minio": "^8.0.4",
    "pinia": "^3.0.1",
    "tdesign-vue-next": "^1.11.2",
    "text": "^0.1.0",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "virtua": "^0.40.3",
    "vite": "^6.2.0",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "^4.5.0",
    "wechat-emoji-parser": "^2.3.1"
  }
}
